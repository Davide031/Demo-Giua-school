{% form_theme form 'tema-new/layout-form.html.twig' %}

<div class="card bg-light border rounded">
  {% if form_help %}
  <div class="card-header bg-dark text-white">{{ form_help|trans|raw  }}</div>
  {% endif %}
  <div class="card-body">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
  </div>
</div>
